# LLM 提供方：openai | qwen_api | local
RAG_LLM_PROVIDER=openai

# OpenAI
OPENAI_API_KEY=your_openai_api_key
RAG_LLM_MODEL=gpt-4o-mini

# Qwen API（DashScope）
DASHSCOPE_API_KEY=your_dashscope_api_key
QWEN_API_MODEL=qwen2.5-coder-7b-instruct

# Local（llama.cpp + GGUF）
QWEN_GGUF_PATH=./models/qwen2.5-coder-7b-instruct-q4_k_m.gguf
LLAMA_CTX=8192
LLAMA_N_GPU_LAYERS=0   # 无GPU就设0；有N卡可调大
LLAMA_THREADS=8        # 按CPU核数调整